    "compilerOptions": 
    {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "outDir": "lib",

        "sourceMap": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "downlevelIteration": true,
        
